    <centralBlocks>
      <Value><![CDATA[[
  {
    "key": "0e41c540-66e8-405f-a604-db1307408bcd",
    "name": "Wednesday 7th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Wednesday 7th September 2022",
    "content": "<table border=\"0\" width=\"743\" style=\"height: 63px;\">\n<tbody>\n<tr style=\"height: 21px;\">\n<td style=\"height: 21px;\">10.00 - 11.00</td>\n<td style=\"height: 21px;\">University Welcome Talk</td>\n<td style=\"height: 21px;\">Halle Lecture Theatre</td>\n</tr>\n</tbody>\n</table>\n<p> </p>"
  },
  {
    "key": "5392c68d-68ef-48e6-a8a7-5e56b9a1f6e2",
    "name": "Thursday 8th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Thursday 8th September 2022",
    "content": "<table border=\"0\" width=\"729\" height=\"18\" style=\"height: 42px;\">\n<tbody>\n<tr style=\"height: 21px;\">\n<td style=\"height: 21px;\">10.00 - 12.00</td>\n<td style=\"height: 21px;\">Induction Talk </td>\n<td style=\"height: 21px;\"><strong>Room 2092</strong></td>\n</tr>\n<tr style=\"height: 21px;\">\n<td style=\"height: 21px;\">13.00 - 14.00</td>\n<td style=\"height: 21px;\">Programme Talk</td>\n<td style=\"height: 21px;\"><strong>Room 2092</strong></td>\n</tr>\n</tbody>\n</table>\n<p> </p>"
  },
  {
    "key": "77192782-3037-49f2-b6f0-5f72a71955f3",
    "name": "Friday 9th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Friday 9th September 2022",
    "content": "<table border=\"1\" style=\"border-collapse: collapse; width: 100%;\">\n<tbody>\n<tr>\n<td style=\"width: 33.3333%;\">12.00 - 12.30</td>\n<td style=\"width: 33.3333%;\">IT Tour</td>\n<td style=\"width: 33.3333%;\">\n<p>Online Session (pre-recorded)</p>\n<p>This session can be watched at the allotted time or at your convenience. <a href=\"https://u.qmu.ac.uk/tour/start/\" title=\"IT Tour session\">Watch the IT Tour session online.</a></p>\n</td>\n</tr>\n<tr>\n<td style=\"width: 33.3333%;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%;\">Discover Library Essentials</td>\n<td style=\"width: 33.3333%;\">\n<p>Details of how to access this session will be emailed to you.</p>\n<p>Please watch this <a href=\"https://www.youtube.com/watch?v=12jCLDKDqe8\" title=\"Library induction (YouTube video)\" data-anchor=\"?v=12jCLDKDqe8\">library induction video</a> before attending this session.</p>\n</td>\n</tr>\n</tbody>\n</table>"
  },
  {
    "key": "f9f4468e-20a0-4824-8b6a-6c45ea83985f",
    "name": "Direct Entrants (years 2-4)",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Direct Entrants (years 2-4)",
    "content": "<p><strong>Monday 5th September</strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 144px;\">\n<tbody>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">09.00 - 09.30</td>\n<td style=\"width: 33.3333%; height: 18px;\">Registration</td>\n<td style=\"width: 33.3333%; height: 18px;\">Main Reception</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%; height: 18px;\">Welcome Talks</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">11.15 - 12.30</td>\n<td style=\"width: 33.3333%; height: 36px;\">Lecture: How are you assessed?</td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 36px;\">Learning Resource Centre (Library) Induction </td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">14.00 - 15.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Programme Talks</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 2170</td>\n</tr>\n</tbody>\n</table>\n<p><strong>Tuesday 6th September</strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 216px;\">\n<tbody>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%; height: 36px;\">Lecture: Academic Writing</td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 72px;\">\n<td style=\"width: 33.3333%; height: 72px;\">11.00 - 12.30</td>\n<td style=\"width: 33.3333%; height: 72px;\">Effective Learning Service: Academic Session</td>\n<td style=\"width: 33.3333%; height: 72px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 72px;\">\n<td style=\"width: 33.3333%; height: 72px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 72px;\">Harvard Referencing with the Learning Resource Centre </td>\n<td style=\"width: 33.3333%; height: 72px;\">Room 3169</td>\n</tr>\n</tbody>\n</table>\n<p><strong>Wednesday 7th September </strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 136px;\">\n<tbody>\n<tr style=\"height: 46px;\">\n<td style=\"width: 33.3333%; height: 46px;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%; height: 46px;\">Lecture: Academic Presentations</td>\n<td style=\"width: 33.3333%; height: 46px;\">\n<p>Room 3169</p>\n</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">11.15 - 12.30</td>\n<td style=\"width: 33.3333%; height: 18px;\">ELS Academic Presentations</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 36px;\">Meet the School Office and Choosing your Modules</td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">14.00 - 15.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Careers Team</td>\n<td style=\"width: 33.3333%; height: 18px;\">Halle Lecture Theatre</td>\n</tr>\n</tbody>\n</table>\n<p><br /><br /></p>"
  }
]]]></Value>
