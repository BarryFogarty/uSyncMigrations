  },
  {
    "key": "5bb01ebe-6f58-4f21-b998-93b69a8be237",
    "name": "Hong Kong Representation in the UK",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Hong Kong Representation in the UK",
    "content": "<p><a href=\"http://edinburgh.china-consulate.gov.cn/eng/\" title=\"Consulate General of the Peoples Republic of China\"><strong>Consulate General of the Peoples Republic of China (Edinburgh)</strong></a><br />55 Corstorphine Road<br />Edinburgh<br />EH12 5QG</p>\n<p>T: +44 131 337 3220<br /><br /></p>"
  }
]]]></Value>
