    "horizontalLinks": "[{\"name\":\"Undergraduate\",\"udi\":\"umb://document/0bff60db2ad540fead1c46abbf0e58b3\"},{\"name\":\"Postgraduate\",\"udi\":\"umb://document/49d44faa2821428fa244a43007d8de10\"},{\"name\":\"Research Degrees\",\"udi\":\"umb://document/33818b0e145c451ea53dd2a4a6aef290\"},{\"name\":\"Continuing Professional Development (CPD) courses\",\"udi\":\"umb://document/d5dfd2a16ef540e6a470bbb71912dce1\"}]"
