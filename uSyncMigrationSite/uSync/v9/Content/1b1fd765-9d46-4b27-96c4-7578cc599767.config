  {
    "key": "b88824e4-3006-45b0-b45a-06fba107b6bf",
    "name": "Horizontal Stories - Student and graduate stories",
    "ncContentTypeAlias": "horizontalStories",
    "componentName": "Student and graduate stories",
    "anchorCode": "studentstories",
    "title": "Student and graduate stories",
    "stories": "[\r\n  {\r\n    \"key\": \"1ba02076-862f-4e62-9f78-7e382286ff48\",\r\n    \"name\": \"Student story - William\",\r\n    \"ncContentTypeAlias\": \"qmu_n_cmp_studentStory\",\r\n    \"componentName\": \"William\",\r\n    \"image\": \"umb://media/b469c6638b5441da9e1a99c54a27e5f7\",\r\n    \"quote\": \"The skills you’ll acquire at QMU, because of the emphasis on practical work and the focus on making students career-ready, meeting employer expectations, mean you have an edge in the market when applying for roles. QMU creates graduates who can hit the ground running.\",\r\n    \"studentsName\": \"William Torrie\",\r\n    \"strapline\": \"BSc (Hons) Psychology\",\r\n    \"currentEmployer\": \"VP of Legal Engagement at Barclays\",\r\n    \"link\": \"[{\\\"name\\\":\\\"Read More\\\",\\\"udi\\\":\\\"umb://document/6271e6d922124293b81fa20404d8b687\\\"}]\"\r\n  },\r\n  {\r\n    \"key\": \"4ae5ed4d-c520-488b-8039-59895e71842d\",\r\n    \"name\": \"Student story - Ben Ferguson\",\r\n    \"ncContentTypeAlias\": \"qmu_n_cmp_studentStory\",\r\n    \"componentName\": \"Ben Ferguson \",\r\n    \"image\": \"umb://media/cf0c37922f9945cebbecc31f93ddc506\",\r\n    \"quote\": \"I chose QMU because I love the modern feel of the campus. It is a very forward-thinking, inclusive and fun environment.\",\r\n    \"studentsName\": \"Ben Ferguson \",\r\n    \"strapline\": \"BA (Hons) Education Studies (Primary)\",\r\n    \"currentEmployer\": \"Primary Teacher\",\r\n    \"link\": \"[{\\\"name\\\":\\\"Read more\\\",\\\"udi\\\":\\\"umb://document/e3d8a0e435db4dd8be9b9716e63c2a86\\\"}]\"\r\n  },\r\n  {\r\n    \"key\": \"b0402aa5-85e7-4410-90ac-c02a916a3f97\",\r\n    \"name\": \"Student story - Alyssa Chan\",\r\n    \"ncContentTypeAlias\": \"qmu_n_cmp_studentStory\",\r\n    \"componentName\": \"Alyssa Chan\",\r\n    \"image\": \"umb://media/f38c3988cca4477e9151077edea4ba90\",\r\n    \"quote\": \"I was looking for something that was quite broad and that would provide me with a lot of options when I graduate. The BSc (Hons) Business Management at QMU offers me that.\",\r\n    \"studentsName\": \"Alyssa Chan\",\r\n    \"strapline\": \"BA/BA (Hons) Business Management\",\r\n    \"currentEmployer\": \"BBC One Star\",\r\n    \"link\": \"[{\\\"name\\\":\\\"Read more\\\",\\\"udi\\\":\\\"umb://document/40760ee57aea4e76a64eae018a556757\\\"}]\"\r\n  },\r\n  {\r\n    \"key\": \"2048912a-6292-4632-9e60-345aa2b40b90\",\r\n    \"name\": \"Student story - Chidi Izunwanne\",\r\n    \"ncContentTypeAlias\": \"qmu_n_cmp_studentStory\",\r\n    \"componentName\": \"Chidi Izunwanne\",\r\n    \"image\": \"umb://media/784484f4f0b1432d94601fa87413ee10\",\r\n    \"quote\": \"My nursing cohort is also close-knit and a great community to be part of. \",\r\n    \"studentsName\": \"Chidi Izunwanne\",\r\n    \"strapline\": \"Master of Nursing (MNurse)\",\r\n    \"currentEmployer\": \"Student\",\r\n    \"link\": \"[{\\\"name\\\":\\\"Read more\\\",\\\"udi\\\":\\\"umb://document/9f53812ac5ec4fd5bf97c63b075957cb\\\"}]\"\r\n  },\r\n  {\r\n    \"key\": \"6515442e-2b3f-408c-afac-573476f4338c\",\r\n    \"name\": \"Student story - Lauren Leisk\",\r\n    \"ncContentTypeAlias\": \"qmu_n_cmp_studentStory\",\r\n    \"componentName\": \"Lauren Leisk\",\r\n    \"image\": \"umb://media/035b6c658ef045c79f2e88147f5bb527\",\r\n    \"quote\": \"I’m a tenant in the University’s Business Innovation Zone which means I continue to receive support and networking opportunities to grow my business.\",\r\n    \"studentsName\": \"Lauren Leisk\",\r\n    \"strapline\": \"BA/BA (Hons) Business Management\",\r\n    \"currentEmployer\": \"Founder and Director at Fodicilious, Ltd. \",\r\n    \"link\": \"[{\\\"name\\\":\\\"Read more\\\",\\\"udi\\\":\\\"umb://document/88defaaaec9941a3abd63ddd8821c0e7\\\"}]\"\r\n  }\r\n]",
    "centreButtonLink": "[{\"name\":\"More student stories\",\"udi\":\"umb://document/bd55567d9b0a42d69e6d8f1947d5bd7f\"}]"
  },
