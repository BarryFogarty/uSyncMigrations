    <centralBlocks>
      <Value><![CDATA[[
  {
    "key": "f38b47ca-ddf1-45f7-89ed-9a88986958da",
    "name": "Monday 5th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Monday 5th September 2022",
    "content": "<table border=\"0\" style=\"width: 770px; height: 97px;\">\n<tbody>\n<tr style=\"height: 75px;\">\n<td style=\"width: 81px; height: 75px;\">10.00 - 10.30</td>\n<td style=\"width: 152px; height: 75px;\">IT Tour</td>\n<td style=\"width: 206px; height: 75px;\">\n<p>Online Session (pre-recorded)</p>\n<p>This session can be watched at the allotted time or at your convenience. <a href=\"https://u.qmu.ac.uk/tour/start/\" title=\"IT Tour session\"><strong>Watch the IT Tour session online</strong></a>.</p>\n</td>\n</tr>\n<tr style=\"height: 22px;\">\n<td style=\"width: 81px; height: 22px;\">11.00 - 12.00</td>\n<td style=\"width: 152px; height: 22px;\">Discover Library Essentials*</td>\n<td style=\"width: 206px; height: 22px;\">\n<p>Details of how to access this session will be emailed to you.</p>\n<p>Please watch this <a href=\"https://www.youtube.com/watch?v=12jCLDKDqe8\" title=\"Library induction (YouTube video)\" data-anchor=\"?v=12jCLDKDqe8\">library induction video</a> before attending this session.</p>\n</td>\n</tr>\n</tbody>\n</table>\n<p> </p>"
  },
  {
    "key": "6ba81765-5506-4f3a-811f-4bdf2079e983",
    "name": "Tuesday 6th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Tuesday 6th September 2022",
    "content": "<table border=\"0\" width=\"768\" height=\"124\" style=\"height: 34px;\">\n<tbody>\n<tr style=\"height: 34px;\">\n<td style=\"width: 87px; height: 34px;\">10.00 - 11.00</td>\n<td style=\"width: 244px; height: 34px;\">University Welcome Talk</td>\n<td style=\"width: 95px; height: 34px;\"><strong>Halle Lecture Theatre</strong></td>\n</tr>\n</tbody>\n</table>\n<p> </p>\n<p> </p>\n<p> </p>"
  },
  {
    "key": "5c8ffd22-c3b4-4804-b968-cfd63b340b54",
    "name": "Wednesday 7th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Wednesday 7th September 2022",
    "content": "<table border=\"0\" width=\"768\" height=\"124\" style=\"height: 95px;\">\n<tbody>\n<tr style=\"height: 19px;\">\n<td style=\"width: 87px; height: 19px;\">13.00 - 14.00</td>\n<td style=\"width: 244px; height: 19px;\">Meet your Programme Leader</td>\n<td style=\"width: 95px; height: 19px;\"><strong>Room 2168</strong></td>\n</tr>\n<tr style=\"height: 19px;\">\n<td style=\"width: 87px; height: 19px;\">14.00 - 14.30</td>\n<td style=\"width: 244px; height: 19px;\">Introduction to Academic Development at QMU</td>\n<td style=\"width: 95px; height: 19px;\"><strong>Room 3089</strong></td>\n</tr>\n<tr style=\"height: 38px;\">\n<td style=\"width: 87px; height: 38px;\">15.00 - 16.00</td>\n<td style=\"width: 244px; height: 38px;\">Meet the Business School Team</td>\n<td style=\"width: 95px; height: 38px;\"><strong>Halle Lecture Theatre</strong></td>\n</tr>\n<tr style=\"height: 19px;\">\n<td style=\"width: 87px; height: 19px;\">16.00 - 16.30</td>\n<td style=\"width: 244px; height: 19px;\">Meet your Personal Academic Tutor</td>\n<td style=\"width: 95px; height: 19px;\"><strong>Room 3189, 3090</strong></td>\n</tr>\n</tbody>\n</table>\n<p> </p>\n<p> </p>"
  },
  {
    "key": "6e3f7711-862d-4109-96f8-98a5517f5f08",
    "name": "Thursday 8th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Thursday 8th September 2022",
    "content": "<table border=\"0\" width=\"768\" height=\"124\">\n<tbody>\n<tr>\n<td style=\"width: 87px;\">09.00 - 17.00</td>\n<td style=\"width: 244px;\">Elementary Food Hygiene Course</td>\n<td style=\"width: 95px;\"><strong>Room 0056</strong></td>\n</tr>\n</tbody>\n</table>\n<p> </p>"
  },
  {
    "key": "8bca7eb2-b752-45a8-9ab4-b818e8230d31",
    "name": "Direct Entrants (years 2-4)",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Direct Entrants (years 2-4)",
    "content": "<p><strong>Monday 5th September</strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 126px;\">\n<tbody>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">09.00 - 09.30</td>\n<td style=\"width: 33.3333%; height: 18px;\">Registration</td>\n<td style=\"width: 33.3333%; height: 18px;\">Main Reception</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%; height: 18px;\">Welcome Talks</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">11.00 - 12.30</td>\n<td style=\"width: 33.3333%; height: 18px;\">Lecture: How are you assessed?</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 36px;\">Learning Resoure Centre (Library) Induction </td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">15.00 - 16.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Careers Team </td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n</tbody>\n</table>\n<p><strong>Tuesday 6th September</strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 108px;\">\n<tbody>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%; height: 18px;\">Lecture: Academic Writing</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">11.15 - 12.30</td>\n<td style=\"width: 33.3333%; height: 36px;\">Effective Learning Service: Academic Session</td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 36px;\">Harvard Referencing with the Learning Resource Centre </td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n</tbody>\n</table>\n<p><strong>Wednesday 7th September </strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 136px;\">\n<tbody>\n<tr style=\"height: 46px;\">\n<td style=\"width: 33.3333%; height: 46px;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%; height: 46px;\">Lecture: Academic Presentations</td>\n<td style=\"width: 33.3333%; height: 46px;\">\n<p>Room 3169</p>\n</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">11.00 - 12.30</td>\n<td style=\"width: 33.3333%; height: 18px;\">ELS Academic Presentations</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 36px;\">Meet the School Office and Choosing your Modules</td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">14.00 - 15.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Meet your Programme Leader</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3170</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">15.00 - 16.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Meet the Business School Team</td>\n<td style=\"width: 33.3333%; height: 18px;\">Halle Lecture Theatre</td>\n</tr>\n</tbody>\n</table>\n<p><br /><br /></p>"
  }
]]]></Value>
