    <centralBlocks>
      <Value><![CDATA[[
  {
    "key": "f22cb8d5-9b8d-4669-ab16-fb8e4099613e",
    "name": "Monday 5th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Monday 5th September 2022",
    "content": "<table border=\"1\" style=\"border-collapse: collapse; width: 100%;\">\n<tbody>\n<tr>\n<td style=\"width: 33.3333%;\">14.00 - 15.00</td>\n<td style=\"width: 33.3333%;\">Programme Talk</td>\n<td style=\"width: 33.3333%;\">Room 0050</td>\n</tr>\n</tbody>\n</table>"
  },
  {
    "key": "04de1631-e2eb-4302-9b4a-efca29932aff",
    "name": "Tuesday 6th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Tuesday 6th September 2022",
    "content": "<p> </p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 18px;\">\n<tbody>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">14.00 - 15.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Meet your Personal Academic Tutor &amp; Understanding your Timetable</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 2177</td>\n</tr>\n</tbody>\n</table>"
  },
  {
    "key": "67e92313-17c6-495e-afee-d2bf5ceba7ef",
    "name": "Wednesday 7th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Wednesday 7th September 2022",
    "content": "<table border=\"1\" style=\"border-collapse: collapse; width: 100%;\">\n<tbody>\n<tr>\n<td style=\"width: 33.3333%;\">10.00 - 10.30</td>\n<td style=\"width: 33.3333%;\">IT Tour</td>\n<td style=\"width: 33.3333%;\">\n<p>Online Session (pre-recorded)</p>\n<p>This session can be watched at the allotted time or at your convenience. <strong>Click </strong><a href=\"https://u.qmu.ac.uk/tour/start/\"><strong>here</strong></a> to watch the session</p>\n</td>\n</tr>\n<tr>\n<td style=\"width: 33.3333%;\">11.00 - 12.00</td>\n<td style=\"width: 33.3333%;\">Discover Library Essentials</td>\n<td style=\"width: 33.3333%;\">\n<p>Details of how to access this session will be emailed to you.</p>\n<p>Please watch this <a href=\"https://www.youtube.com/watch?v=12jCLDKDqe8\">short video</a> before attending this session.</p>\n</td>\n</tr>\n</tbody>\n</table>"
  },
  {
    "key": "20996126-0e65-43e8-8080-62c0884b3c65",
    "name": "Thursday 8th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Thursday 8th September 2022",
    "content": "<table border=\"1\" style=\"border-collapse: collapse; width: 100%;\">\n<tbody>\n<tr>\n<td style=\"width: 33.3333%;\">11.00 - 12.00</td>\n<td style=\"width: 33.3333%;\">University Welcome Talk</td>\n<td style=\"width: 33.3333%;\">Halle Lecture Theatre</td>\n</tr>\n<tr>\n<td style=\"width: 33.3333%;\">14.00 0 15.00</td>\n<td style=\"width: 33.3333%;\">Catch Up with Programme Leader</td>\n<td style=\"width: 33.3333%;\">Room 0050</td>\n</tr>\n</tbody>\n</table>"
  },
  {
    "key": "43bdb573-18b4-4d63-942c-f978ef235b89",
    "name": "Direct Entrants (years 2-4)",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Direct Entrants (years 2-4)",
    "content": "<p><strong>Monday 5th September</strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 108px;\">\n<tbody>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">09.00 - 09.30</td>\n<td style=\"width: 33.3333%; height: 18px;\">Registration</td>\n<td style=\"width: 33.3333%; height: 18px;\">Main Reception</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%; height: 18px;\">Welcome Talks</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">11.15 - 12.30</td>\n<td style=\"width: 33.3333%; height: 18px;\">Lecture: How are you assessed?</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 36px;\">Learning Resource Centre (Library) Induction </td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">14.00 - 15.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Programme Talks</td>\n<td style=\"width: 33.3333%; height: 18px;\">Rooms 2169</td>\n</tr>\n</tbody>\n</table>\n<p><strong>Tuesday 6th September</strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 234px;\">\n<tbody>\n<tr style=\"height: 54px;\">\n<td style=\"width: 33.3333%; height: 54px;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%; height: 54px;\">Lecture: Academic Writing</td>\n<td style=\"width: 33.3333%; height: 54px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 90px;\">\n<td style=\"width: 33.3333%; height: 90px;\">11.00 - 12.30</td>\n<td style=\"width: 33.3333%; height: 90px;\">Effective Learning Service: Academic Session</td>\n<td style=\"width: 33.3333%; height: 90px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 90px;\">\n<td style=\"width: 33.3333%; height: 90px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 90px;\">Harvard Referencing with the Learning Resource Centre </td>\n<td style=\"width: 33.3333%; height: 90px;\">Room 0047</td>\n</tr>\n</tbody>\n</table>\n<p><strong>Wednesday 7th September </strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 117px;\">\n<tbody>\n<tr style=\"height: 46px;\">\n<td style=\"width: 33.3333%; height: 45px;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%; height: 45px;\">Mini-lecture and what to do with lectures</td>\n<td style=\"width: 33.3333%; height: 45px;\">\n<p>Room 2108</p>\n</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">11.00 - 12.30</td>\n<td style=\"width: 33.3333%; height: 18px;\">ELS Academic Presentations</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 36px;\">Meet the School Office and Choosing your Modules</td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">14.00 - 15.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Careers Team</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n</tbody>\n</table>\n<p><br /><br /></p>"
  }
]]]></Value>
