    <centralBlocks>
      <Value><![CDATA[[
  {
    "key": "3ee5b34d-fa45-4e67-8087-5a15b3440f86",
    "name": "Tuesday 6th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Tuesday 6th September 2022",
    "content": "<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 54px;\">\n<tbody>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">10.00 - 11.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Programme Talk</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 0066</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 36px;\">Academic Work and Assignments Talk</td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 0066</td>\n</tr>\n</tbody>\n</table>"
  },
  {
    "key": "864425db-8546-41cd-ad74-45a79cf6cdf2",
    "name": "Wednesday 7th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Wednesday 7th September 2022",
    "content": "<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 188px;\">\n<tbody>\n<tr style=\"height: 96px;\">\n<td style=\"width: 33.3333%; height: 96px;\">14.00 - 14.30</td>\n<td style=\"width: 33.3333%; height: 96px;\">IT Tour</td>\n<td style=\"width: 33.3333%; height: 96px;\">\n<p>Online Session (pre-recorded)</p>\n<p>This session can be watched at the allotted time or at your convenience. <a href=\"https://u.qmu.ac.uk/tour/start/\" title=\"IT Tour session\">Watch the IT Tour session online</a>.</p>\n</td>\n</tr>\n<tr style=\"height: 46px;\">\n<td style=\"width: 33.3333%; height: 46px;\">15.00 - 16.00</td>\n<td style=\"width: 33.3333%; height: 46px;\">Discover Library Essentials**</td>\n<td style=\"width: 33.3333%; height: 46px;\">\n<p>Details of how to access this session will be emailed to you.</p>\n<p>Please watch this <a href=\"https://www.youtube.com/watch?v=12jCLDKDqe8\" title=\"Library induction (YouTube video)\" data-anchor=\"?v=12jCLDKDqe8\">library induction video</a> before attending this session.</p>\n</td>\n</tr>\n</tbody>\n</table>\n<p> </p>"
  },
  {
    "key": "0e5cbed2-4862-4a4f-8960-8a7197659d6e",
    "name": "Thursday 8th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Thursday 8th September 2022",
    "content": "<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 36px;\">\n<tbody>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">11.00 - 12.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">University Welcome Talk</td>\n<td style=\"width: 33.3333%; height: 18px;\">Halle Lecture Theatre</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">13.00 - 16.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Outdoor Induction and Welcome</td>\n<td style=\"width: 33.3333%; height: 18px;\">Maggie's Outdoor Wood (Meet outside Students' Union)</td>\n</tr>\n</tbody>\n</table>"
  },
  {
    "key": "cbf510b6-2f88-4a9d-b860-9948efc59881",
    "name": "Direct Entrants (years 2-4)",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Direct Entrants (years 2-4)",
    "content": "<p><strong>Monday 5th September</strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 144px;\">\n<tbody>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">09.00 - 09.30</td>\n<td style=\"width: 33.3333%; height: 18px;\">Registration</td>\n<td style=\"width: 33.3333%; height: 18px;\">Main Reception</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%; height: 18px;\">Welcome Talks</td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">11.15 - 12.30</td>\n<td style=\"width: 33.3333%; height: 36px;\">Lecture: How are you assessed?</td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 36px;\">\n<td style=\"width: 33.3333%; height: 36px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 36px;\">Learning Resource Centre (Library) Induction </td>\n<td style=\"width: 33.3333%; height: 36px;\">Room 3169</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">14.00 - 15.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Programme Talks</td>\n<td style=\"width: 33.3333%; height: 18px;\">Rooms 2171</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">15.00 - 16.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Careers Team </td>\n<td style=\"width: 33.3333%; height: 18px;\">Room 3169</td>\n</tr>\n</tbody>\n</table>\n<p><strong>Tuesday 6th September</strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%;\">\n<tbody>\n<tr>\n<td style=\"width: 33.3333%;\">09.30 - 10.45</td>\n<td style=\"width: 33.3333%;\">Intro to Academic Writing and research Methods for Education Studies</td>\n<td style=\"width: 33.3333%;\">Room 0049</td>\n</tr>\n<tr>\n<td style=\"width: 33.3333%;\">11.00 - 12.30</td>\n<td style=\"width: 33.3333%;\">Effective Learning Service: Academic Session</td>\n<td style=\"width: 33.3333%;\">Room 3169</td>\n</tr>\n<tr>\n<td style=\"width: 33.3333%;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%;\">Harvard Referencing with the Learning Resource Centre </td>\n<td style=\"width: 33.3333%;\">Room 0047</td>\n</tr>\n</tbody>\n</table>\n<p><strong>Wednesday 7th September </strong></p>\n<table border=\"1\" style=\"border-collapse: collapse; width: 100%;\">\n<tbody>\n<tr>\n<td style=\"width: 33.3333%;\">\n<p>09.30 - 10.45</p>\n<p> </p>\n</td>\n<td style=\"width: 33.3333%;\">Lecture: Academic Presentations</td>\n<td style=\"width: 33.3333%;\">Room 3169</td>\n</tr>\n<tr>\n<td style=\"width: 33.3333%;\">\n<p>11.00 - 12.30</p>\n</td>\n<td style=\"width: 33.3333%;\">ELS Presentations</td>\n<td style=\"width: 33.3333%;\">Room 3169</td>\n</tr>\n</tbody>\n</table>\n<p><br /><br /></p>"
  }
]]]></Value>
