    <centralBlocks>
      <Value><![CDATA[[
  {
    "key": "237a92f5-5e79-470c-816a-097db4cae42f",
    "name": "Monday 5th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Monday 5th September 2022",
    "content": "<table border=\"1\" style=\"border-collapse: collapse; width: 100%;\">\n<tbody>\n<tr>\n<td style=\"width: 33.3333%;\">14.00 - 15.30</td>\n<td style=\"width: 33.3333%;\">Pre-Placement Talk</td>\n<td style=\"width: 33.3333%;\"><strong>Halle Lecture Theatre</strong></td>\n</tr>\n</tbody>\n</table>\n<p> </p>"
  },
  {
    "key": "3ee89c79-1adb-4550-a875-5b950d079d4e",
    "name": "Wednesday 7th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Wednesday 7th September 2022",
    "content": "<table border=\"0\">\n<tbody>\n<tr>\n<td>10:00 - 11.00</td>\n<td>University Welcome Talk</td>\n<td>Halle Lecture Theatre</td>\n</tr>\n<tr>\n<td>13.00 - 14.00</td>\n<td>Introduction to your Podiatry Degree</td>\n<td>\n<p>Room 2090</p>\n</td>\n</tr>\n<tr>\n<td>14.00 - 15.00</td>\n<td>Talk with your Programme Leader &amp; Podiatry Programme Structure</td>\n<td>Room 2090</td>\n</tr>\n<tr>\n<td>15.00 - 16:00</td>\n<td>\n<p>Meet the Team and Fellow Students</p>\n</td>\n<td>Room 2090</td>\n</tr>\n</tbody>\n</table>\n<p> </p>"
  },
  {
    "key": "2e12d268-2a14-4f88-94c5-2ed231f19275",
    "name": "Thursday 8th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Thursday 8th September 2022",
    "content": "<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 57px;\">\n<tbody>\n<tr style=\"height: 21px;\">\n<td style=\"width: 33.3333%; height: 21px;\">10.00 - 11.00</td>\n<td style=\"width: 33.3333%; height: 21px;\">Meet your Personal Academic Tutor</td>\n<td style=\"width: 33.3333%; height: 21px;\">Online Session via Collaborate</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">12.00 - 12.30</td>\n<td style=\"width: 33.3333%; height: 18px;\">IT Tour</td>\n<td style=\"width: 33.3333%; height: 18px;\">\n<p>Online Session (pre-recorded)</p>\n<p>This session can be watched at the allotted time or at your convenience. <a href=\"https://u.qmu.ac.uk/tour/start/\" title=\"IT Tour session\">Watch the IT Tour session online.</a></p>\n</td>\n</tr>\n<tr style=\"height: 18px;\">\n<td style=\"width: 33.3333%; height: 18px;\">13.00 - 14.00</td>\n<td style=\"width: 33.3333%; height: 18px;\">Discover Library Essentials</td>\n<td style=\"width: 33.3333%; height: 18px;\">\n<p>Details of how to access this session will be emailed to you.</p>\n<p>Please watch this <a href=\"https://www.youtube.com/watch?v=12jCLDKDqe8\" title=\"Library induction (YouTube video)\" data-anchor=\"?v=12jCLDKDqe8\">library induction video</a> before attending this session.</p>\n</td>\n</tr>\n</tbody>\n</table>"
  },
  {
    "key": "d8d50206-dbe8-4ba7-9833-c8a54b9bcb84",
    "name": "Friday 9th September 2022",
    "ncContentTypeAlias": "centralBlocks",
    "title": "Friday 9th September 2022",
    "content": "<table border=\"1\" style=\"border-collapse: collapse; width: 100%; height: 21px;\">\n<tbody>\n<tr style=\"height: 21px;\">\n<td style=\"width: 33.3333%; height: 21px;\">12.00 - 13.30</td>\n<td style=\"width: 33.3333%; height: 21px;\">Placement Passport Introduction*</td>\n<td style=\"width: 33.3333%; height: 21px;\">Room 2090</td>\n</tr>\n</tbody>\n</table>\n<p>*Students to bring their own laptop. Alternatively a very limited number will be available to borrow from the library for the session.</p>"
  }
]]]></Value>
