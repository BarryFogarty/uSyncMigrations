.overlapping-text .inner-box {
    text-align: center;
    background-color: white;
    margin-top: 10px;
    margin-bottom: 10px;
    position: relative;
    top: -80px;
    z-index: 10;
}

.image-background-container {
    background-size: cover;
    background-position: center;
    height: 500px;
    position: relative;
}

.image-background-container p.cut-in-text {
    position: absolute;
    bottom: 0;
    left: 0;
    margin-bottom: 0;
    font-weight: 600;
    text-align: center;
    background: white;
    width: 50%;
    font-size: 32px;
    padding: 10px;
}

.container.image-grid .image-link {
    margin-left: 10%;
    margin-right: 10%;
    margin-bottom: 5%;
}

.container.image-grid .image-link.centre-link {
    margin-bottom: 10%;
}

.container.image-grid .image-link .image {
    position: relative;
    display: block;
    overflow: hidden;
}

.container.image-grid .image-link .image:focus-visible {
    outline: 4px solid #5376A1;
}

.container.image-grid .image-link .image img {
    aspect-ratio: 1.6860;
    object-fit: cover;
    object-position: center;
    width: 100%;
    -moz-transition: transform 0.3s ease-in-out;
    -o-transition: transform 0.3s ease-in-out;
    -webkit-transition: -webkit-transform 0.3s ease-in-out;
    transition: transform 0.3s ease-in-out;
    -moz-transition: opacity, 0.25s;
    -o-transition: opacity, 0.25s;
    -webkit-transition: opacity, 0.25s;
    transition: opacity, 0.25s;
}

.container.image-grid .image-link .image:hover img {
    -moz-transform: scale(1.08);
    -ms-transform: scale(1.08);
    -o-transform: scale(1.08);
    -webkit-transform: scale(1.08);
    transform: scale(1.08);
    opacity: 0.3;
}

.container.image-grid .image-link.centre-link .image img {
    aspect-ratio: 1.2462;
}

.container.image-grid .image-link .image .text {
    position: absolute;
    left: 0;
    bottom: 0;
    text-align: center;
}

.container.image-grid .image-link .image span {
    color: white;
    background-color: #374a5c;
    font-weight: 600;
    font-size: 24px;
    box-shadow: 10px 0px #374a5c, -10px 0px #374a5c;
    box-decoration-break: clone;
    padding-top: 5px;
    padding-bottom: 5px;
    margin-left: 10px;
    margin-right: 10px;
    -webkit-box-decoration-break : clone;
}

.container.image-grid .image-link a.image:focus-visible span {
    background: #fd0;
    color: #0b0c0c;
}

