@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.BandedArticle>
@using ContentModels = Umbraco.Web.PublishedModels;
@using Umbraco.Web

@{
    Layout = "Master.cshtml";

    <link rel="stylesheet" type="text/css" href="/css/banded-article.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>

}

<header role="banner" class="banner">
    <div class="item">
        <span style="background-image: url(/media/8d885b436829fc8/youcandoitfromhome.jpg)" class="bg-cover-img" role="img" aria-label="A laptop on someone's desk at home beside a set of headphones and some notepads"> </span>
        <div class="text-overlay">
            <div class="container">
                <h1 class="bannerTitle">You can do <strong>IT</strong> at Home</h1>

                <div class="banner-intro">
                    <p>If you are working or studying away from campus there are a range of services available to you.&nbsp; These links provide access to all the key services you will require.&nbsp; A link is also provided for access to the help desk should you require support during office hours.</p>
                </div>
            </div>

        </div>
    </div>
</header>

